<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="2.5">
	
	<servlet>
		<servlet-name>HomeServlet</servlet-name>
		<servlet-class>projectatlast.frontend.HomeServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>HomeServlet</servlet-name>
		<url-pattern>/home</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GroupServletTest</servlet-name>
		<servlet-class>projectatlast.GroupServletTest</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>GroupServletTest</servlet-name>
		<url-pattern>/groupservlettest</url-pattern>
	</servlet-mapping>
	
	
	<servlet>
		<servlet-name>PostLoginServlet</servlet-name>
		<servlet-class>projectatlast.student.PostLoginServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>PostLoginServlet</servlet-name>
		<url-pattern>/postlogin</url-pattern>
	</servlet-mapping>
	
	

	<welcome-file-list>
		<welcome-file>home</welcome-file>
	</welcome-file-list>
</web-app>
